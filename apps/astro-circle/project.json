{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "astro-circle",
  "projectType": "application",
  "sourceRoot": "apps/astro-circle/src",
  "tags": [
    "astro:astro-circle"
  ],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec astro --root apps/astro-circle build"
      }
    },
    "add": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec astro --root apps/astro-circle add"
      }
    },
    "info": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec astro --root apps/astro-circle info"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec astro --root apps/astro-circle check"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec astro --root apps/astro-circle dev"
      }
    }
  }
}