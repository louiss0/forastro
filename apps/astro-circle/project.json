{
  "name": "astro-circle",
  "root": "apps/astro-circle",
  "sourceRoot": "apps/astro-circle/src",
  "projectType": "application",
  "tags": ["type:app", "scope:astro-circle"],
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm astro dev",
        "cwd": "apps/astro-circle"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "command": "pnpm astro build",
        "cwd": "apps/astro-circle"
      }
    },
    "check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm astro check",
        "cwd": "apps/astro-circle"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm eslint \"apps/astro-circle/**/*.{ts,tsx,astro}\" --max-warnings=0"
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm tsc -p apps/astro-circle/tsconfig.json --noEmit"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "apps/astro-circle/.vitest"
      }
    }
  }
}
