{
  "name": "docs",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "command": "vitepress build packages/..//docs"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitepress dev packages/..//docs"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitepress serve packages/..//docs"
      }
    }
  },
  "tags": []
}