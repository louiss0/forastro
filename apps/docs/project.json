{
  "name": "docs",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/docs/src",
  "tags": [
    "nx:docs"
  ],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec astro --root apps/docs build"
      }
    },
    "add": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec astro --root apps/docs add"
      }
    },
    "info": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec astro --root apps/docs info"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec astro --root apps/docs check"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec astro --root apps/docs dev"
      }
    },
    "sync": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec astro --root apps/docs sync"
      }
    }
  }
}