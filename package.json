{
  "name": "@forastro/monorepo",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "preversion": "pnpm -w -r run build",
    "lint:affected": "pnpm nx affected -t lint --parallel --exclude=astro-nx",
    "test:affected": "pnpm nx affected -t test --parallel --exclude=astro-nx",
    "build:affected": "pnpm nx affected -t build --parallel",
    "check:affected": "pnpm nx affected -t check --parallel",
    "ci": "pnpm -w install --frozen-lockfile  pnpm run lint:affected  pnpm run test:affected  pnpm run check:affected  pnpm run build:affected"
  },
  "private": true,
