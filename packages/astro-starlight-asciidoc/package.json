{
  "name": "@forastro/starlight-asciidoc",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "license": "MIT",
  "sideEffects": false,
  "files": ["dist"],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./integration": {
      "types": "./dist/integration.d.ts",
      "import": "./dist/integration.js"
    },
    "./markdoc": {
      "types": "./dist/markdoc/index.d.ts",
      "import": "./dist/markdoc/index.js"
    },
    "./components/*": "./dist/components/*"
  },
  "peerDependencies": {
    "astro": "^4.0.0",
    "@astrojs/starlight": "^0.25.0",
    "@astrojs/markdoc": "^0.11.0",
    "@forastro/asciidoc": "^0.6.0"
  },
  "dependencies": {
    "markdoc": "^0.1.3"
  },
  "devDependencies": {
    "typescript": "^5.5.0",
    "tsup": "^8.0.0",
    "vitest": "^1.6.0",
    "cpy-cli": "^5.0.0",
    "rimraf": "^6.0.0"
  },
  "scripts": {
    "build": "rimraf dist && tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "test": "vitest run"
  },
  "publishConfig": {
    "access": "public"
  }
}

