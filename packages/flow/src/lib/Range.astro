---
import { iterateRange} from "@forastro/utilities";

type Props = Parameters<typeof iterateRange>[1]

const { start, step = 1, stop, inclusive } = Astro.props;
---

<Fragment
  set:html={iterateRange((...args) => Astro.slots.render("default", args), {
    start,
    stop,
    step,
    inclusive
  })}
/>
