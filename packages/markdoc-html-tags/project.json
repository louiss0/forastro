{
    "name": "markdoc-html-tags",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "packages/markdoc-html-tags",
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "nx:run-commands",
            "outputs": [
                "{options.outputPath}"
            ],
            "options": {
                "command": "tsup packages/markdoc-html-tags"
            }
        },
        "test": {
            "executor": "@nx/vite:test",
            "outputs": [
                "coverage/packages/markdoc-html-tags"
            ],
            "options": {
                "passWithNoTests": true,
                "reportsDirectory": "../../coverage/packages/markdoc-html-tags"
            }
        }
    },
    "tags": []
}