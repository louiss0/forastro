{
  "dev": {
    "executor": "astro-nx:dev",
    "options": {
      "root": ".",
      "packageManager": "pnpm",
      "port": 4321,
      "host": "localhost",
      "open": false,
      "verbose": false
    }
  },
  "build": {
    "executor": "astro-nx:build",
    "options": {
      "root": ".",
      "packageManager": "pnpm",
      "outputPath": "dist",
      "verbose": false
    },
    "outputs": ["{projectRoot}/dist"]
  },
  "preview": {
    "executor": "astro-nx:preview",
    "options": {
      "root": ".",
      "packageManager": "pnpm",
      "port": 4173,
      "host": "localhost",
      "open": false,
      "verbose": false
    },
    "dependsOn": ["build"]
  },
  "check": {
    "executor": "astro-nx:check",
    "options": {
      "root": ".",
      "packageManager": "pnpm",
      "watch": false,
      "verbose": false
    }
  }
}
