    "cypress": "^14.5.2",
    "eslint": "9.31.0",
    "nx": "21.2.3",
    "semantic-release": "^22.0.12",
    "typescript": "5.8.3",
    "vitest": "^3.2.4",
    "tsup": "^8.5.0"
