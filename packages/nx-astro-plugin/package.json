    "@nx/js": "21.2.3",
    "@nx/plugin": "21.2.3",
    "@nx/vite": "^21.2.3",
    "@nx/workspace": "21.2.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.3.5",
    "@semantic-release/npm": "^11.0.3",
    "@types/node": "24.0.13",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
